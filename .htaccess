Options +FollowSymlinks
RewriteEngine on
RewriteBase /
RewriteRule ^logs/\d\d\d\d\-\d\d-\d\d*$ logs/index.cgi [L]
RewriteRule ^pure-lang/\d\d\d\d\-\d\d-\d\d*$ pure-lang/index.cgi [L]
RewriteRule ^.osurb/\d\d\d\d\-\d\d-\d\d .osurb/index.cgi [L]
RewriteRule ^logs/today$ logs/today.php [L]
RewriteRule ^pure-lang/today$ pure-lang/today.php [L]
RewriteRule ^ip.* http://yano.nu/ip/ [R=301,L]
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://yanovich.net/$1 [R=301,L]
